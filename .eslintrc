{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "linebreak-style": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/forbid-prop-types": 0,
    "import/extensions": 0,
    "import/no-named-as-default-member": 0,
    "import/no-named-as-default": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/media-has-caption": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-mixed-operators": 0,
    "no-else-return": 0,
    "no-param-reassign": 0
  }
}
